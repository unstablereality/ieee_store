<tr>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.security %></td>
  <td class = "edit-button"><%= link_to image_tag("edit-icon.png"), edit_user_path(user) %></td>
  <td class = "delete-button"><%= link_to image_tag("delete-button.png"), user, :method => :delete, :confirm => "Are you sure?" %>
</tr>
