<!DOCTYPE html>
<html>
<head>
  <title>IEEE Parts Store</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class = "flash_message">
  <% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :class => "flash_#{name}" %>
  <% end %>
  <%= link_to "Home", root_path %>
</div>
<%= yield %>

</body>
</html>
